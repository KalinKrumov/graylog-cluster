GRAYLOG_HOME=/opt/graylog
GRAYLOG_JAVA_OPTS=-Dlog4j.configuration=file:///etc/graylog/server/log4j.xml -Djava.library.path=$GRAYLOG_HOME/lib -Djava.net.preferIPv4Stack=true -Xms768m -Xmx768m -XX:PermSize=128m -XX:MaxPermSize=256m -server -XX:+ResizeTLAB -XX:+UseConcMarkSweepGC -XX:+CMSConcurrentMTEnabled -XX:+CMSClassUnloadingEnabled -XX:+UseParNewGC
